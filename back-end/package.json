    "dotenv": "^16.0.3",
    "eslint": "6.8.0",
    "eslint-config-trybe-backend": "1.0.3",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "nodemon": "^2.0.20"
  }
}
